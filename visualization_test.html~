<!doctype html>
<html>
<head>
  <title>Graph 3D cloud with colored dots</title>

  <style>
    body {font: 10pt arial;}
  </style>
  <script src="plot_data.js"></script>
  <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/vis/4.7.0/vis.min.js"></script>
  <script type="text/javascript">
    var graph = null;
    var data  = null;

    // Called when the Visualization API is loaded.
    function drawVisualization(data) {
      // create the data table.
      data = new vis.Graph3d.setData(data)


      // specify options
      var options = {
        width:  '600px',
        height: '600px',
        style: 'dot-color',
        showPerspective: true,
        showGrid: true,
        keepAspectRatio: true,
        verticalRatio: 1.0,
        legendLabel: 'Weight',
        cameraPosition: {
          horizontal: -10,
          vertical: 10,
          distance: 1.8
        }
      };

      // create our graph
      var container = document.getElementById('mygraph');
      graph = new vis.Graph3d(container, data, options);
    }
  </script>
</head>

<body onload="plot_data.js"
      onload="drawVisualization(part)">
<div id="mygraph"></div>

<div id="info"></div>
</body>
</html>
